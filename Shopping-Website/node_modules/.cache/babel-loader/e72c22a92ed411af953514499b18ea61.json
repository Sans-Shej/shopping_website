{"ast":null,"code":"export default {\n  data() {\n    return {\n      currentCardBackground: Math.floor(Math.random() * 25 + 1),\n      // just for fun :D\n      cardName: \"\",\n      cardNumber: \"\",\n      cardMonth: \"\",\n      cardYear: \"\",\n      cardCvv: \"\",\n      minCardYear: new Date().getFullYear(),\n      amexCardMask: \"#### ###### #####\",\n      otherCardMask: \"#### #### #### ####\",\n      cardNumberTemp: \"\",\n      isCardFlipped: false,\n      focusElementStyle: null,\n      isInputFocused: false\n    };\n  },\n\n  mounted() {\n    this.cardNumberTemp = this.otherCardMask;\n    document.getElementById(\"cardNumber\").focus();\n  },\n\n  computed: {\n    getCardType() {\n      let number = this.cardNumber;\n      let re = new RegExp(\"^4\");\n      if (number.match(re) != null) return \"visa\";\n      re = new RegExp(\"^(34|37)\");\n      if (number.match(re) != null) return \"amex\";\n      re = new RegExp(\"^5[1-5]\");\n      if (number.match(re) != null) return \"mastercard\";\n      re = new RegExp(\"^6011\");\n      if (number.match(re) != null) return \"discover\";\n      re = new RegExp('^9792');\n      if (number.match(re) != null) return 'troy';\n      return \"visa\"; // default type\n    },\n\n    generateCardNumberMask() {\n      return this.getCardType === \"amex\" ? this.amexCardMask : this.otherCardMask;\n    },\n\n    minCardMonth() {\n      if (this.cardYear === this.minCardYear) return new Date().getMonth() + 1;\n      return 1;\n    }\n\n  },\n  watch: {\n    cardYear() {\n      if (this.cardMonth < this.minCardMonth) {\n        this.cardMonth = \"\";\n      }\n    }\n\n  },\n  methods: {\n    flipCard(status) {\n      this.isCardFlipped = status;\n    },\n\n    focusInput(e) {\n      this.isInputFocused = true;\n      let targetRef = e.target.dataset.ref;\n      let target = this.$refs[targetRef];\n      this.focusElementStyle = {\n        width: `${target.offsetWidth}px`,\n        height: `${target.offsetHeight}px`,\n        transform: `translateX(${target.offsetLeft}px) translateY(${target.offsetTop}px)`\n      };\n    },\n\n    blurInput() {\n      let vm = this;\n      setTimeout(() => {\n        if (!vm.isInputFocused) {\n          vm.focusElementStyle = null;\n        }\n      }, 300);\n      vm.isInputFocused = false;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAmSA,eAAe;EACbA,IAAI,GAAG;IACL,OAAO;MACLC,qBAAqB,EAAEC,IAAI,CAACC,KAAL,CAAWD,IAAI,CAACE,MAAL,KAAe,EAAf,GAAoB,CAA/B,CADlB;MACqD;MAC1DC,QAAQ,EAAE,EAFL;MAGLC,UAAU,EAAE,EAHP;MAILC,SAAS,EAAE,EAJN;MAKLC,QAAQ,EAAE,EALL;MAMLC,OAAO,EAAE,EANJ;MAOLC,WAAW,EAAE,IAAIC,IAAJ,GAAWC,WAAX,EAPR;MAQLC,YAAY,EAAE,mBART;MASLC,aAAa,EAAE,qBATV;MAULC,cAAc,EAAE,EAVX;MAWLC,aAAa,EAAE,KAXV;MAYLC,iBAAiB,EAAE,IAZd;MAaLC,cAAc,EAAE;IAbX,CAAP;EAeD,CAjBY;;EAkBbC,OAAO,GAAG;IACR,KAAKJ,cAAL,GAAsB,KAAKD,aAA3B;IACAM,QAAQ,CAACC,cAAT,CAAwB,YAAxB,EAAsCC,KAAtC;EACD,CArBY;;EAsBbC,QAAQ,EAAE;IACRC,WAAU,GAAK;MACb,IAAIC,MAAK,GAAI,KAAKnB,UAAlB;MACA,IAAIoB,EAAC,GAAI,IAAIC,MAAJ,CAAW,IAAX,CAAT;MACA,IAAIF,MAAM,CAACG,KAAP,CAAaF,EAAb,KAAoB,IAAxB,EAA8B,OAAO,MAAP;MAE9BA,EAAC,GAAI,IAAIC,MAAJ,CAAW,UAAX,CAAL;MACA,IAAIF,MAAM,CAACG,KAAP,CAAaF,EAAb,KAAoB,IAAxB,EAA8B,OAAO,MAAP;MAE9BA,EAAC,GAAI,IAAIC,MAAJ,CAAW,SAAX,CAAL;MACA,IAAIF,MAAM,CAACG,KAAP,CAAaF,EAAb,KAAoB,IAAxB,EAA8B,OAAO,YAAP;MAE9BA,EAAC,GAAI,IAAIC,MAAJ,CAAW,OAAX,CAAL;MACA,IAAIF,MAAM,CAACG,KAAP,CAAaF,EAAb,KAAoB,IAAxB,EAA8B,OAAO,UAAP;MAE9BA,EAAC,GAAI,IAAIC,MAAJ,CAAW,OAAX,CAAL;MACA,IAAIF,MAAM,CAACG,KAAP,CAAaF,EAAb,KAAoB,IAAxB,EAA8B,OAAO,MAAP;MAE9B,OAAO,MAAP,CAjBa,CAiBE;IAChB,CAnBO;;IAoBVG,sBAAqB,GAAK;MACzB,OAAO,KAAKL,WAAL,KAAqB,MAArB,GAA8B,KAAKX,YAAnC,GAAkD,KAAKC,aAA9D;IACE,CAtBO;;IAuBRgB,YAAW,GAAK;MACd,IAAI,KAAKtB,QAAL,KAAkB,KAAKE,WAA3B,EAAwC,OAAO,IAAIC,IAAJ,GAAWoB,QAAX,KAAwB,CAA/B;MACxC,OAAO,CAAP;IACF;;EA1BQ,CAtBG;EAkDbC,KAAK,EAAE;IACLxB,QAAO,GAAK;MACV,IAAI,KAAKD,SAAL,GAAiB,KAAKuB,YAA1B,EAAwC;QACtC,KAAKvB,SAAL,GAAiB,EAAjB;MACF;IACF;;EALK,CAlDM;EAyDb0B,OAAO,EAAE;IACPC,QAAO,CAAGC,MAAH,EAAW;MAChB,KAAKnB,aAAL,GAAqBmB,MAArB;IACD,CAHM;;IAIPC,UAAS,CAAGC,CAAH,EAAM;MACb,KAAKnB,cAAL,GAAsB,IAAtB;MACA,IAAIoB,SAAQ,GAAID,CAAC,CAACE,MAAF,CAASC,OAAT,CAAiBC,GAAjC;MACA,IAAIF,MAAK,GAAI,KAAKG,KAAL,CAAWJ,SAAX,CAAb;MACA,KAAKrB,iBAAL,GAAyB;QACvB0B,KAAK,EAAG,GAAEJ,MAAM,CAACK,WAAY,IADN;QAEvBC,MAAM,EAAG,GAAEN,MAAM,CAACO,YAAa,IAFR;QAGvBC,SAAS,EAAG,cAAaR,MAAM,CAACS,UAAW,kBAAiBT,MAAM,CAACU,SAAU;MAHtD,CAAzB;IAKD,CAbM;;IAcPC,SAAS,GAAG;MACV,IAAIC,EAAC,GAAI,IAAT;MACAC,UAAU,CAAC,MAAM;QACf,IAAI,CAACD,EAAE,CAACjC,cAAR,EAAwB;UACtBiC,EAAE,CAAClC,iBAAH,GAAuB,IAAvB;QACF;MACD,CAJS,EAIP,GAJO,CAAV;MAKAkC,EAAE,CAACjC,cAAH,GAAoB,KAApB;IACF;;EAtBO;AAzDI,CAAf","names":["data","currentCardBackground","Math","floor","random","cardName","cardNumber","cardMonth","cardYear","cardCvv","minCardYear","Date","getFullYear","amexCardMask","otherCardMask","cardNumberTemp","isCardFlipped","focusElementStyle","isInputFocused","mounted","document","getElementById","focus","computed","getCardType","number","re","RegExp","match","generateCardNumberMask","minCardMonth","getMonth","watch","methods","flipCard","status","focusInput","e","targetRef","target","dataset","ref","$refs","width","offsetWidth","height","offsetHeight","transform","offsetLeft","offsetTop","blurInput","vm","setTimeout"],"sourceRoot":"","sources":["C:\\Users\\Sans\\Downloads\\Shopping-Website\\Shopping-Website\\src\\views\\Payment.vue"],"sourcesContent":["<!-- <template>\r\n    <div>\r\n    <div>\r\n          <div className=\"card-details-container\">\r\n            <h3 className=\"ha2\">ENTER YOUR CARD DETAILS</h3>\r\n            <form action=\"\" className=\"card-detail\">\r\n              <input type=\"number\" name=\"cardnumber\"  value={user.cardnumber} onChange={handleChange}  placeholder=\"CARD NUMBER\"  />\r\n              <input type=\"month\" name=\"month\" value={user.month} onChange={handleChange}  placeholder=\"MONTH\" />\r\n              <input type=\"number\" name=\"year\" value={user.year} onChange={handleChange}  placeholder=\"YEAR\" />\r\n              <br />\r\n              <input type=\"text\" className=\"card-holder-input\" name=\"cardholder\" value={user.cardholder} onChange={handleChange}  placeholder=\"CARD HOLDER\" />\r\n              <input type=\"text\" name=\"cvv\" value={user.cvv} onChange={handleChange} placeholder=\"CVV2 SECURITY CODE\" />\r\n            </form>\r\n            \r\n          </div>\r\n  \r\n          <div className=\"cards\">\r\n            <div className=\"pr\">\r\n              <div className=\"price\">\r\n                <p>TOTAL : RS {{ cartCheckoutPrice }}</p>\r\n              </div>\r\n              <button onClick={purchase}>\r\n                CONTINUE\r\n              </button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      \r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n   import { mapGetters } from \"vuex\";\r\n   export default {\r\n    computed: {\r\n    ...mapGetters([\r\n      \"cartCheckoutPrice\",\r\n    ]),\r\n  },\r\n   };\r\n  </script>\r\n\r\n  <style>\r\n.cards{\r\n    display: flex;\r\n    justify-content: space-between;\r\n    background-color: white;\r\n    width: 100%;\r\n    height: 80px;\r\n    position: fixed;\r\n    margin-top: 5%;\r\n\r\n}\r\n.pr{\r\n    display: flex;\r\n    margin-left: 69%;\r\n}\r\nspan{\r\n    font-size: 12px;\r\n    font-weight: bold;\r\n}\r\n.img{\r\n    border: 1px solid rgb(204, 202, 202);\r\n    width: 8%;\r\n    margin-top: 60px;\r\n    margin-left: 7%;\r\n\r\n}\r\nform>input{\r\n    border: none;\r\n    border-bottom: 1px solid rgb(196, 193, 193);\r\n    width: 20%;\r\n    height: 25px;\r\n    font-size: 10px;\r\n    font-weight: bold;\r\n    margin-right: 30px;\r\n  \r\n    margin-top: 50px;\r\n}\r\n.ha2{\r\n    margin-left: 7%;\r\n    font-size: 90%;\r\n\r\n}\r\n.card-details-container{\r\n   border: 1px solid black;\r\n    width: 50%;\r\n    margin: auto;\r\n    margin-top: 10%;\r\n}\r\n.card-holder-input{\r\n    margin-left: 10%;\r\n    width: 20%;\r\n}\r\n@media only screen and (min-width:375px) and (max-width:768px){\r\n \r\n    .cards{\r\n        margin-top: 490px;\r\n    }\r\n    .pr{\r\n        margin-left: 50%;\r\n    }\r\n}\r\n\r\n@media only screen and (min-width:0px) and (max-width:390px){\r\n    .cards{\r\n        margin-top: 245px;\r\n        border-top: 2px solid black;\r\n    }\r\n    .pr{\r\n        margin-left: 15%;\r\n    }\r\n    .card-details-container>img{\r\n        width: 90px;\r\n    }\r\n    form>input{\r\n        width: 35%;\r\n        font-size: 12px;\r\n    }\r\n}\r\n\r\n.btn-continue{\r\n    margin-left: 75%;\r\n    position: relative;\r\n}\r\n  </style> -->\r\n\r\n\r\n<template>\r\n    <div class=\"wrapper\" id=\"app\">\r\n    <div class=\"card-form\">\r\n      <div class=\"card-list\">\r\n        <div class=\"card-item\" v-bind:class=\"{ '-active' : isCardFlipped }\">\r\n          <div class=\"card-item__side -front\">\r\n            <div class=\"card-item__focus\" v-bind:class=\"{'-active' : focusElementStyle }\" v-bind:style=\"focusElementStyle\" ref=\"focusElement\"></div>\r\n            <div class=\"card-item__cover\">\r\n              <img\r\n              v-bind:src=\"'https://raw.githubusercontent.com/muhammederdem/credit-card-form/master/src/assets/images/' + currentCardBackground + '.jpeg'\" class=\"card-item__bg\">\r\n            </div>\r\n            \r\n            <div class=\"card-item__wrapper\">\r\n              <div class=\"card-item__top\">\r\n                <img src=\"https://raw.githubusercontent.com/muhammederdem/credit-card-form/master/src/assets/images/chip.png\" class=\"card-item__chip\">\r\n                <div class=\"card-item__type\">\r\n                  <transition name=\"slide-fade-up\">\r\n                    <img v-bind:src=\"'https://raw.githubusercontent.com/muhammederdem/credit-card-form/master/src/assets/images/' + getCardType + '.png'\" v-if=\"getCardType\" v-bind:key=\"getCardType\" alt=\"\" class=\"card-item__typeImg\">\r\n                  </transition>\r\n                </div>\r\n              </div>\r\n              <label for=\"cardNumber\" class=\"card-item__number\" ref=\"cardNumber\">\r\n                <template v-if=\"getCardType === 'amex'\">\r\n                 <span v-for=\"(n, $index) in amexCardMask\" :key=\"$index\">\r\n                  <transition name=\"slide-fade-up\">\r\n                    <div\r\n                      class=\"card-item__numberItem\"\r\n                      v-if=\"$index > 4 && $index < 14 && cardNumber.length > $index && n.trim() !== ''\"\r\n                    >*</div>\r\n                    <div class=\"card-item__numberItem\"\r\n                      :class=\"{ '-active' : n.trim() === '' }\"\r\n                      :key=\"$index\" v-else-if=\"cardNumber.length > $index\">\r\n                      {{cardNumber[$index]}}\r\n                    </div>\r\n                    <div\r\n                      class=\"card-item__numberItem\"\r\n                      :class=\"{ '-active' : n.trim() === '' }\"\r\n                      v-else\r\n                      :key=\"$index + 1\"\r\n                    >{{n}}</div>\r\n                  </transition>\r\n                </span>\r\n                </template>\r\n\r\n                <template v-else>\r\n                  <span v-for=\"(n, $index) in otherCardMask\" :key=\"$index\">\r\n                    <transition name=\"slide-fade-up\">\r\n                      <div\r\n                        class=\"card-item__numberItem\"\r\n                        v-if=\"$index > 4 && $index < 15 && cardNumber.length > $index && n.trim() !== ''\"\r\n                      >*</div>\r\n                      <div class=\"card-item__numberItem\"\r\n                        :class=\"{ '-active' : n.trim() === '' }\"\r\n                        :key=\"$index\" v-else-if=\"cardNumber.length > $index\">\r\n                        {{cardNumber[$index]}}\r\n                      </div>\r\n                      <div\r\n                        class=\"card-item__numberItem\"\r\n                        :class=\"{ '-active' : n.trim() === '' }\"\r\n                        v-else\r\n                        :key=\"$index + 1\"\r\n                      >{{n}}</div>\r\n                    </transition>\r\n                  </span>\r\n                </template>\r\n              </label>\r\n              <div class=\"card-item__content\">\r\n                <label for=\"cardName\" class=\"card-item__info\" ref=\"cardName\">\r\n                  <div class=\"card-item__holder\">Card Holder</div>\r\n                  <transition name=\"slide-fade-up\">\r\n                    <div class=\"card-item__name\" v-if=\"cardName.length\" key=\"1\">\r\n                      <transition-group name=\"slide-fade-right\">\r\n                        <span class=\"card-item__nameItem\" v-for=\"(n, $index) in cardName.replace(/\\s\\s+/g, ' ')\" v-if=\"$index === $index\" v-bind:key=\"$index + 1\">{{n}}</span>\r\n                      </transition-group>\r\n                    </div>\r\n                    <div class=\"card-item__name\" v-else key=\"2\">Full Name</div>\r\n                  </transition>\r\n                </label>\r\n                <div class=\"card-item__date\" ref=\"cardDate\">\r\n                  <label for=\"cardMonth\" class=\"card-item__dateTitle\">Expires</label>\r\n                  <label for=\"cardMonth\" class=\"card-item__dateItem\">\r\n                    <transition name=\"slide-fade-up\">\r\n                      <span v-if=\"cardMonth\" v-bind:key=\"cardMonth\">{{cardMonth}}</span>\r\n                      <span v-else key=\"2\">MM</span>\r\n                    </transition>\r\n                  </label>\r\n                  /\r\n                  <label for=\"cardYear\" class=\"card-item__dateItem\">\r\n                    <transition name=\"slide-fade-up\">\r\n                      <span v-if=\"cardYear\" v-bind:key=\"cardYear\">{{String(cardYear).slice(2,4)}}</span>\r\n                      <span v-else key=\"2\">YY</span>\r\n                    </transition>\r\n                  </label>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <div class=\"card-item__side -back\">\r\n            <div class=\"card-item__cover\">\r\n              <img\r\n              v-bind:src=\"'https://raw.githubusercontent.com/muhammederdem/credit-card-form/master/src/assets/images/' + currentCardBackground + '.jpeg'\" class=\"card-item__bg\">\r\n            </div>\r\n            <div class=\"card-item__band\"></div>\r\n            <div class=\"card-item__cvv\">\r\n                <div class=\"card-item__cvvTitle\">CVV</div>\r\n                <div class=\"card-item__cvvBand\">\r\n                  <span v-for=\"(n, $index) in cardCvv\" :key=\"$index\">\r\n                    *\r\n                  </span>\r\n\r\n              </div>\r\n                <div class=\"card-item__type\">\r\n                    <img v-bind:src=\"'https://raw.githubusercontent.com/muhammederdem/credit-card-form/master/src/assets/images/' + getCardType + '.png'\" v-if=\"getCardType\" class=\"card-item__typeImg\">\r\n                </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"card-form__inner\">\r\n        <div class=\"card-input\">\r\n          <label for=\"cardNumber\" class=\"card-input__label\">Card Number</label>\r\n          <input type=\"text\" id=\"cardNumber\" class=\"card-input__input\" v-mask=\"generateCardNumberMask\" v-model=\"cardNumber\" v-on:focus=\"focusInput\" v-on:blur=\"blurInput\" data-ref=\"cardNumber\" autocomplete=\"off\">\r\n        </div>\r\n        <div class=\"card-input\">\r\n          <label for=\"cardName\" class=\"card-input__label\">Card Holders</label>\r\n          <input type=\"text\" id=\"cardName\" class=\"card-input__input\" v-model=\"cardName\" v-on:focus=\"focusInput\" v-on:blur=\"blurInput\" data-ref=\"cardName\" autocomplete=\"off\">\r\n        </div>\r\n        <div class=\"card-form__row\">\r\n          <div class=\"card-form__col\">\r\n            <div class=\"card-form__group\">\r\n              <label for=\"cardMonth\" class=\"card-input__label\">Expiration Date</label>\r\n              <select class=\"card-input__input -select\" id=\"cardMonth\" v-model=\"cardMonth\" v-on:focus=\"focusInput\" v-on:blur=\"blurInput\" data-ref=\"cardDate\">\r\n                <option value=\"\" disabled selected>Month</option>\r\n                <option v-bind:value=\"n < 10 ? '0' + n : n\" v-for=\"n in 12\" v-bind:disabled=\"n < minCardMonth\" v-bind:key=\"n\">\r\n                    {{n < 10 ? '0' + n : n}}\r\n                </option>\r\n              </select>\r\n              <select class=\"card-input__input -select\" id=\"cardYear\" v-model=\"cardYear\" v-on:focus=\"focusInput\" v-on:blur=\"blurInput\" data-ref=\"cardDate\">\r\n                <option value=\"\" disabled selected>Year</option>\r\n                <option v-bind:value=\"$index + minCardYear\" v-for=\"(n, $index) in 12\" v-bind:key=\"n\">\r\n                    {{$index + minCardYear}}\r\n                </option>\r\n              </select>\r\n            </div>\r\n          </div>\r\n          <div class=\"card-form__col -cvv\">\r\n            <div class=\"card-input\">\r\n              <label for=\"cardCvv\" class=\"card-input__label\">CVV</label>\r\n              <input type=\"text\" class=\"card-input__input\" id=\"cardCvv\" v-mask=\"'####'\" maxlength=\"4\" v-model=\"cardCvv\" v-on:focus=\"flipCard(true)\" v-on:blur=\"flipCard(false)\" autocomplete=\"off\">\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <button class=\"card-form__button\">\r\n          Submit\r\n        </button>\r\n      </div>\r\n    </div>\r\n    \r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      currentCardBackground: Math.floor(Math.random()* 25 + 1), // just for fun :D\r\n      cardName: \"\",\r\n      cardNumber: \"\",\r\n      cardMonth: \"\",\r\n      cardYear: \"\",\r\n      cardCvv: \"\",\r\n      minCardYear: new Date().getFullYear(),\r\n      amexCardMask: \"#### ###### #####\",\r\n      otherCardMask: \"#### #### #### ####\",\r\n      cardNumberTemp: \"\",\r\n      isCardFlipped: false,\r\n      focusElementStyle: null,\r\n      isInputFocused: false\r\n    };\r\n  },\r\n  mounted() {\r\n    this.cardNumberTemp = this.otherCardMask;\r\n    document.getElementById(\"cardNumber\").focus();\r\n  },\r\n  computed: {\r\n    getCardType () {\r\n      let number = this.cardNumber;\r\n      let re = new RegExp(\"^4\");\r\n      if (number.match(re) != null) return \"visa\";\r\n\r\n      re = new RegExp(\"^(34|37)\");\r\n      if (number.match(re) != null) return \"amex\";\r\n\r\n      re = new RegExp(\"^5[1-5]\");\r\n      if (number.match(re) != null) return \"mastercard\";\r\n\r\n      re = new RegExp(\"^6011\");\r\n      if (number.match(re) != null) return \"discover\";\r\n      \r\n      re = new RegExp('^9792')\r\n      if (number.match(re) != null) return 'troy'\r\n\r\n      return \"visa\"; // default type\r\n    },\r\n\t\tgenerateCardNumberMask () {\r\n\t\t\treturn this.getCardType === \"amex\" ? this.amexCardMask : this.otherCardMask;\r\n    },\r\n    minCardMonth () {\r\n      if (this.cardYear === this.minCardYear) return new Date().getMonth() + 1;\r\n      return 1;\r\n    }\r\n  },\r\n  watch: {\r\n    cardYear () {\r\n      if (this.cardMonth < this.minCardMonth) {\r\n        this.cardMonth = \"\";\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    flipCard (status) {\r\n      this.isCardFlipped = status;\r\n    },\r\n    focusInput (e) {\r\n      this.isInputFocused = true;\r\n      let targetRef = e.target.dataset.ref;\r\n      let target = this.$refs[targetRef];\r\n      this.focusElementStyle = {\r\n        width: `${target.offsetWidth}px`,\r\n        height: `${target.offsetHeight}px`,\r\n        transform: `translateX(${target.offsetLeft}px) translateY(${target.offsetTop}px)`\r\